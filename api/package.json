{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "lts",
    "@nestjs/common": "lts",
    "@nestjs/config": "lts",
    "@nestjs/core": "lts",
    "@nestjs/jwt": "lts",
    "@nestjs/passport": "lts",
    "@nestjs/platform-express": "lts",
    "@nestjs/platform-socket.io": "lts",
    "@nestjs/swagger": "lts",
    "@nestjs/typeorm": "lts",
    "@nestjs/websockets": "lts",
    "bcrypt": "lts",
    "class-transformer": "lts",
    "class-validator": "lts",
    "nodemailer": "lts",
    "passport": "lts",
    "passport-jwt": "lts",
    "passport-local": "lts",
    "pg": "lts",
    "reflect-metadata": "lts",
    "rimraf": "lts",
    "rxjs": "lts",
    "typeorm": "lts"
  },
  "devDependencies": {
    "@nestjs/cli": "lts",
    "@nestjs/schematics": "lts",
    "@nestjs/testing": "lts",
    "@types/bcrypt": "lts",
    "@types/express": "lts",
    "@types/jest": "lts",
    "@types/multer": "lts",
    "@types/node": "lts",
    "@types/nodemailer": "lts",
    "@types/passport-jwt": "lts",
    "@types/passport-local": "lts",
    "@typescript-eslint/eslint-plugin": "lts",
    "@typescript-eslint/parser": "lts",
    "eslint": "lts",
    "jest": "lts",
    "otplib": "lts",
    "source-map-support": "lts",
    "ts-jest": "lts",
    "ts-loader": "lts",
    "ts-node": "lts",
    "tsconfig-paths": "lts",
    "typescript": "lts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
